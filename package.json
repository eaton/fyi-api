{
  "name": "fyi-api",
  "version": "0.0.1",
  "description": "Eaton.FYI's data migration toolbox and content API.",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">18.14.0"
  },
  "scripts": {
    "demo": "ts-node-esm src/index.ts ",
    "build": "npm run clean; npm run compile",
    "test": "ava",
    "clean": "rm -rf ./dist;rm *.tsbuildinfo",
    "compile": "tsc -p ./tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "files": [
    "dist/source/**/*",
    "dist/docs/**/*"
  ],
  "prettier": {
    "editorconfig": true,
    "semi": true,
    "singleQuote": true,
    "indent_style": "space",
    "indent_size": 2,
    "bracketSpacing": true
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/https://github.com/eaton/fyi-api.git"
  },
  "keywords": [
    "url",
    "scraping",
    "webcrawling"
  ],
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/https://github.com/eaton/fyi-api/issues"
  },
  "homepage": "https://github.com/eaton/fyi-api",
  "dependencies": {
    "@autogram/url-tools": "^2.5.4",
    "@ava/typescript": "^4.1.0",
    "@contentful/rich-text-from-markdown": "^15.16.8",
    "@contentful/rich-text-html-renderer": "^16.3.0",
    "@contentful/rich-text-plain-text-renderer": "^16.0.7",
    "@portabletext/to-html": "^2.0.5",
    "@portabletext/toolkit": "^2.0.10",
    "@salesforce/ts-types": "^2.0.7",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/block-tools": "^3.19.0",
    "@sanity/schema": "^3.18.1",
    "@sanity/types": "^3.19.0",
    "@sindresorhus/is": "^6.0.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@twitter-api-v2/plugin-rate-limit": "^1.1.0",
    "@types/fs-extra": "^11.0.2",
    "@types/got": "^9.6.12",
    "@types/html-to-text": "^9.0.2",
    "@types/jsdom": "^21.1.4",
    "@types/ndjson": "^2.0.3",
    "@types/node": "^20.2.5",
    "@types/object-hash": "^3.0.4",
    "@types/prompts": "^2.4.5",
    "@types/turndown": "^5.0.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "arangojs": "^8.4.1",
    "autogram-sanity": "github:autogram-is/autogram-sanity#v2",
    "autohome": "github:autogram-is/autogram-sanity#v2",
    "ava": "^5.3.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "cheerio-json-mapper": "^1.0.3",
    "crawlee": "^3.5.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "entities": "^4.5.0",
    "eslint": "^8.42.0",
    "fast-csv": "^4.3.6",
    "fast-glob": "^3.3.1",
    "file-type": "^18.5.0",
    "fs-extra": "^11.1.1",
    "get-urls": "^12.1.0",
    "got": "^13.0.0",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "humanize-url": "^3.0.0",
    "jsdom": "^22.1.0",
    "ky": "^1.0.1",
    "mangler": "github:eaton/mangler#main",
    "marked": "^9.1.4",
    "mysql2": "^3.6.1",
    "ndjson": "^2.0.0",
    "neat-csv": "^7.0.0",
    "node-pinboard": "^2.0.1",
    "object-hash": "^3.0.0",
    "open": "^9.1.0",
    "p-throttle": "^5.1.0",
    "playwright": "^1.38.1",
    "playwright-extra": "^4.3.6",
    "prettier": "^2.8.8",
    "protocolify": "^4.0.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "remeda": "^1.27.1",
    "superstruct": "^1.0.3",
    "textile-js": "^2.1.1",
    "ts-node": "^10.9.1",
    "tumblr.js": "^4.0.1",
    "turndown": "^7.1.2",
    "twitter-api-v2": "^1.15.1",
    "twitter-archive-reader": "^7.2.0",
    "type-fest": "^4.3.3",
    "typescript": "^5.2.0",
    "uuid": "^9.0.1",
    "yaml": "^2.3.2",
    "zod": "^3.22.4"
  }
}

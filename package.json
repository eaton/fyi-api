{
  "name": "eaton-fyi-db",
  "version": "0.0.1",
  "description": "Helper classes for bulk migration and data mangling",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">18.14.0"
  },
  "scripts": {
    "demo": "ts-node-esm src/index.ts ",
    "build": "npm run clean; npm run compile",
    "clean": "rm -rf ./dist;rm *.tsbuildinfo",
    "compile": "tsc -p ./tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "files": [
    "dist/source/**/*",
    "dist/docs/**/*"
  ],
  "xo": {
    "space": true,
    "prettier": true,
    "bracketSpacing": true,
    "rules": {
      "new-cap": "off",
      "max-params": "off"
    }
  },
  "prettier": {
    "editorconfig": true,
    "semi": true,
    "singleQuote": true,
    "indent_style": "space",
    "indent_size": 2,
    "bracketSpacing": true
  },
  "ava": {
    "files": [
      "tests/*"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": "dist/tests/"
      },
      "compile": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/https://github.com/eaton/fyi-db.git"
  },
  "keywords": [
    "url",
    "scraping",
    "webcrawling"
  ],
  "author": "Jeff Eaton <jeff@autogram.is> (http://eaton.fyi/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/https://github.com/eaton/fyi-db/issues"
  },
  "homepage": "https://github.com/eaton/fyi-db",
  "dependencies": {
    "@autogram/url-tools": "^2.5.4",
    "@salesforce/ts-types": "^2.0.7",
    "@sindresorhus/is": "^6.0.0",
    "@types/cheerio": "^0.22.32",
    "@types/fs-extra": "^11.0.2",
    "@types/got": "^9.6.12",
    "@types/html-to-text": "^9.0.2",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.2.5",
    "@types/object-hash": "^3.0.4",
    "@types/turndown": "^5.0.2",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "arangojs": "^8.4.1",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "entities": "^4.5.0",
    "eslint": "^8.42.0",
    "fast-csv": "^4.3.6",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.7",
    "got": "^13.0.0",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "minimatch": "^9.0.3",
    "mysql": "^2.18.1",
    "node-pinboard": "^2.0.1",
    "object-hash": "^3.0.0",
    "prettier": "^2.8.8",
    "superstruct": "^1.0.3",
    "ts-node": "^10.9.1",
    "tumblr.js": "^4.0.1",
    "turndown": "^7.1.2",
    "twitter-archive-reader": "^7.2.0",
    "typescript": "^5.2.0",
    "uuid": "^9.0.1",
    "windows-1252": "^3.0.4",
    "xmldom-ts": "^0.3.1",
    "xpath-ts": "^1.3.13"
  }
}
